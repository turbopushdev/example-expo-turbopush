{
  "name": "expo-53-newarch",
  "version": "1.0.2",
  "main": "expo-router/entry",
  "scripts": {
    "prebuild": "expo prebuild",
    "start": "expo start",
    "android": "yarn prebuild && expo run:android",
    "ios": "yarn prebuild && expo run:ios",
    "web": "expo start --web",
    "deploy": "npx expo export -p web && npx eas-cli@latest deploy",
    "codepush:normal": "yarn turbopush release-expo cli-official ios 1.0.2 -d Production -r 100",
    "codepush:android:normal": "yarn turbopush release-expo cli-official-android android 1.0.2 -d Production -r 100",
    "codepush:android": "yarn codepush:android:normal --mandatory",
    "codepush": "yarn codepush:normal --mandatory"
  },
  "dependencies": {
    "@expo/config-plugins": "~10.1.1",
    "@turbopush/react-native-code-push": "^10.2.3",
    "expo": "^53.0.0",
    "expo-application": "~6.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "nativewind": "^4.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@turbopush/cli": "^1.0.25",
    "@turbopush/turbopush-expo-plugin": "^1.0.0",
    "@types/react": "~19.0.10",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
